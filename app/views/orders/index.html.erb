<table class="table">
  <thead>
    <tr>
      <th scope="col">name</th>
      <th scope="col">price</th>
    </tr>
  </thead>
  <tbody>
  <% @orders.each do |order| %>
  
    <tr>
      <th scope="row"><%= order.product.name %></th>
      <td><%= order.product.price %></td>
    </tr>
      <% end %>
  </tbody>
</table>